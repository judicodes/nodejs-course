{
  "extends": ["plugin:prettier/recommended", "prettier", "eslint:recommended"],
  "plugins": ["prettier", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "project": "/users/judi/workspace/nodejs-course/4-natours/starter/tsconfig.json"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error",
    "no-var": "error"
  }
}
